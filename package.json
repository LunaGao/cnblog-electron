{
  "name": "cnblogs",
  "version": "0.2.0",
  "description": "cnblogs",
  "main": "main.js",
  "dependencies": {
    "electron-debug": "^0.1.1",
    "encoding": "^0.1.12",
    "flat-ui": "^2.1.1",
    "jquery": "^2.2.0",
    "node-localstorage": "^1.1.2",
    "node-rsa": "^0.2.30",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^2.0.0-rc4",
    "request": "^2.67.0",
    "toastr": "^2.1.2",
    "ursa": "^0.9.1"
  },
  "devDependencies": {
    "babel-preset-react": "^6.3.13",
    "electron-packager": "^5.2.0"
  },
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager ./ cnblogs --platform=darwin --arch=x64 --out ~/Desktop/cnblogs --version 0.36.7 --overwrite --icon=./app/img/app-icon.icns --asar=true",
    "package-linux64": "electron-packager ./ cnblogs --platform=linux --arch=x64 --out ~/Desktop/cnblogs --version 0.36.7 --overwrite --asar=true",
    "package-linux32": "electron-packager ./ cnblogs --platform=linux --arch=ia32 --out ~/Desktop/cnblogs --version 0.36.7 --overwrite --asar=true",
    "package-win64": "electron-packager ./ cnblogs --platform=win32 --arch=x64 --out ~/Desktop/cnblogs --version 0.36.7 --overwrite --icon=./app/img/app-icon.ico --asar=true --ignore=cnblogs.app",
    "package-win32": "electron-packager ./ cnblogs --platform=win32 --arch=ia32 --out ~/Desktop/cnblogs --version 0.36.7 --overwrite --icon=./app/img/app-icon.ico --asar=true --ignore=cnblogs.app"
  },
  "keywords": [
    "Cnblogs"
  ],
  "author": "LunaGao",
  "license": "MIT"
}
